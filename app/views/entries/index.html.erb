


<% @entries.each do |entry| %>
  <%= link_to "upvote", like_entry_path(entry), method: :put %>
<%= link_to "downvote", dislike_entry_path(entry), method: :put %>
<% end %>


  <% for entry in @entries %>
  <h2><%= entry.title %></h2>
    <p><%= entry.date %></p>
    <p><%= entry.contents %></p>
<% end %>


<h2><%= link_to(entry.title, entry_path(entry)) %></h2>

<%= link_to("New entry", new_entry_path) %>